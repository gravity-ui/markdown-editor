{
  "name": "@doc-tools/yfm-editor",
  "version": "3.2.0",
  "description": "YFM wysiwyg editor",
  "scripts": {
    "dev": "npm run storybook:start",
    "clean": "gulp clean",
    "build": "gulp",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "storybook:start": "TS_NODE_PROJECT=.storybook/tsconfig.json start-storybook -p 8888 -c .storybook",
    "storybook:build": "TS_NODE_PROJECT=.storybook/tsconfig.json build-storybook -c .storybook -o storybook-static",
    "lint": "run-p -cs lint:*",
    "lint:js": "eslint \"{src,demo}/**/*.{js,jsx,ts,tsx}\"",
    "lint:styles": "stylelint \"{src,demo}/**/*.{css,scss}\"",
    "lint:prettier": "prettier --check \"{src,demo}/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "test": "jest",
    "test:cov": "jest --coverage",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yandex-cloud/yfm-editor"
  },
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/esm/index.d.ts",
  "files": [
    "assets",
    "build"
  ],
  "keywords": [
    "yfm",
    "wysiwyg",
    "prosemirror"
  ],
  "author": "Data UI Team <data-ui@yandex-team.ru>",
  "license": "MIT",
  "dependencies": {
    "@bem-react/classname": "^1.5.12",
    "@types/is-number": "^7.0.1",
    "@types/markdown-it": "^12.2.3",
    "is-number": "^7.0.0",
    "markdown-it-color": "^2.1.1",
    "markdown-it-ins": "^3.0.1",
    "markdown-it-katex": "2.0.3",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-sub": "^1.0.0",
    "prosemirror-codemark": "0.4.0",
    "prosemirror-commands": "1.3.0",
    "prosemirror-dropcursor": "1.5.0",
    "prosemirror-history": "1.3.0",
    "prosemirror-inputrules": "1.2.0",
    "prosemirror-keymap": "1.2.0",
    "prosemirror-model": "1.18.1",
    "prosemirror-schema-list": "1.2.0",
    "prosemirror-state": "1.4.1",
    "prosemirror-transform": "1.6.0",
    "prosemirror-utils": "1.0.0-0",
    "prosemirror-view": "1.26.2",
    "react-use": "^17.3.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@doc-tools/transform": "2.16.0",
    "@gravity-ui/eslint-config": "1.0.2",
    "@gravity-ui/prettier-config": "1.0.1",
    "@gravity-ui/stylelint-config": "1.0.1",
    "@gravity-ui/tsconfig": "1.0.0",
    "@gravity-ui/uikit": "3.1.0",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.5.10",
    "@types/gulp": "4.0.9",
    "@types/gulp-sass": "5.0.0",
    "@types/jest": "^27.0.3",
    "@types/katex": "0.5.0",
    "@types/lodash": "^4.14.177",
    "@types/react": "17.0.50",
    "@types/react-dom": "17.0.17",
    "@types/rimraf": "3.0.2",
    "eslint": "8.24.0",
    "gulp": "4.0.2",
    "gulp-cli": "2.3.0",
    "gulp-concat": "2.6.1",
    "gulp-replace": "1.1.3",
    "gulp-sass": "5.1.0",
    "gulp-typescript": "5.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-css-modules": "^2.1.0",
    "markdown-it-testgen": "^0.1.6",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "prosemirror-test-builder": "1.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "sass": "^1.43.4",
    "sass-loader": "10.2.1",
    "stylelint": "14.13.0",
    "svg-sprite-loader": "6.0.11",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@doc-tools/transform": "^2.16.0",
    "@gravity-ui/uikit": "^3.1.0",
    "lodash": "^4.17.20",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "lint-staged": {
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix --quiet",
      "prettier --write"
    ],
    "*.{md,json,yaml,yml}": "prettier --write"
  }
}
